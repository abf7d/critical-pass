{
    "name": "explorer-web",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "sourceRoot": "apps/explorer-web/src",
    "prefix": "critical-pass",
    "targets": {
        "build": {
            "executor": "@angular-devkit/build-angular:browser",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/apps/explorer-web",
                "index": "apps/explorer-web/src/index.html",
                "main": "apps/explorer-web/src/main.ts",
                "polyfills": ["zone.js"],
                "tsConfig": "apps/explorer-web/tsconfig.app.json",
                "inlineStyleLanguage": "scss",
                "assets": ["apps/explorer-web/src/favicon.ico", "apps/explorer-web/src/assets"],
                "styles": ["apps/explorer-web/src/styles.scss"],
                "scripts": []
            },
            "configurations": {
                "production": {
                    "budgets": [
                        {
                            "type": "initial",
                            "maximumWarning": "500kb",
                            "maximumError": "1mb"
                        },
                        {
                            "type": "anyComponentStyle",
                            "maximumWarning": "2kb",
                            "maximumError": "4kb"
                        }
                    ],
                    "outputHashing": "all",
                    "fileReplacements": [
                        {
                            "replace": "libs/shared/environments/src/lib/environment.ts",
                            "with": "libs/shared/environments/src/lib/environment.prod.ts"
                        }
                    ]
                },
                "development": {
                    "buildOptimizer": false,
                    "optimization": false,
                    "vendorChunk": true,
                    "extractLicenses": false,
                    "sourceMap": true,
                    "namedChunks": true
                }
            },
            "defaultConfiguration": "production"
        },
        "serve": {
            "executor": "@angular-devkit/build-angular:dev-server",
            "configurations": {
                "production": {
                    "browserTarget": "explorer-web:build:production"
                },
                "development": {
                    "browserTarget": "explorer-web:build:development"
                }
            },
            "defaultConfiguration": "development"
        },
        "extract-i18n": {
            "executor": "@angular-devkit/build-angular:extract-i18n",
            "options": {
                "browserTarget": "explorer-web:build"
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["apps/explorer-web/**/*.ts", "apps/explorer-web/**/*.html"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "apps/explorer-web/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "component-test": {
            "executor": "@nrwl/cypress:cypress",
            "options": {
                "cypressConfig": "apps/explorer-web/cypress.config.ts",
                "testingType": "component",
                "skipServe": true,
                "devServerTarget": "explorer-web:build"
            }
        }
    },
    "tags": []
}
